<?xml version="1.0" encoding="utf-8"?>
<granar>
	<!-- General parameters -->
	<planttype param="2" /> <!-- 1 = monocot / 2 = dicot -->
	<randomness param="2" /> <!-- 0 = no radomness / 3 = high randomness-->
	<secondarygrowth param="1"/> <!-- 0 = no secondary growth / 1 = sec growth -->
	
	<!-- Stele layers -->
	<!-- Order defines the ranking of the tissue radialy -->
	<!-- cell_diameter = cell width [mm]; layer_diameter = diameter of the stele  -->
	<stele cell_diameter="0.01" n_layers="1" layer_diameter = "0.25" order="1"/>
	<cambium cell_diameter="0.01" n_layers = "1" order = "2"/>
	<phloem cell_diameter="0.01" n_layers = "1" order = "2.5"/>
	<pericycle cell_diameter="0.01" n_layers="1"  order="3"/>
	<endodermis cell_diameter="0.02" n_layers="1"  order="4"/>
	<!-- Cortex layers -->
	<!-- n_layer = number of layer -->
	<inner_cortex cell_diameter="0.02" n_layers="2"  order="5"/>
	<cortex cell_diameter="0.04" n_layers="3"  order="6"/>
	<!--outer_cortex cell_diameter="0.01" n_layers="1"  order="6"-->
	<exodermis cell_diameter="0.03" n_layers="1" order="7"/>
	<epidermis cell_diameter="0.01" n_layers="1" order="8"/>

	<!-- Vessels-->
	<!-- max_size = maximum cell diameter  [mm] -->
	<!-- n_files = number of metaxylem elements -->
	<!-- ratio = ratio of proto vs. metaxylem elements -->
	<xylem max_size="0.05" cell_diameter = "0.01" n_files="12" order="1.5" ratio ="3.3" />
	
	<!-- Other cell types -->
	<aerenchyma proportion="0.2" n_files="2" type = "2" /> <!-- 1 = strip / 2 = patch -->
	<!-- hair length = "0.07" n_files="3" -->
	<inter_cellular_space size= "0.002" ratio= "0.01" />
	<!-- pith layer_diameter="0.03" cell_diameter="0.01" -->
</granar>